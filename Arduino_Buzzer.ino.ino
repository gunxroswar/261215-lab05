   if(stage > 5) stage = 1;
   Serial.printf("stage: %d\n", stage);
   lastPress = millis()+500;
}

void setup(){
  Serial.begin(115200);
  pinMode(buzzer1, OUTPUT);
  pinMode(buzzer2, OUTPUT);
  pinMode(button, INPUT);
  digitalWrite(buzzer1, HIGH);
  digitalWrite(buzzer2, HIGH);
  attachInterrupt(button, IO_INT_ISR, RISING);
  timer0 = timerBegin(0, 80, true);
  timer1 = timerBegin(1, 80, true);
  timer2 = timerBegin(2, 80, true);
  timer3 = timerBegin(3, 80, true);
  timerAttachInterrupt(timer0, &onTimer0, true);
  timerAttachInterrupt(timer1, &onTimer1, true);
  timerAttachInterrupt(timer2, &onTimer2, true);
  timerAttachInterrupt(timer3, &onTimer3, true);
 
  timerAlarmWrite(timer0, 1000000/buzzer1Frequency[0], true);
  timerAlarmWrite(timer1, 1000000/buzzer1Duration[0], true);
  timerAlarmWrite(timer2, 1000000/buzzer2Frequency[0], true);
  timerAlarmWrite(timer3, 1000000/buzzer2Duration[0], true);

  timerAlarmEnable(timer0);
  timerAlarmEnable(timer1);
  timerAlarmEnable(timer2);
  timerAlarmEnable(timer3);
}

void loop(){
  if(buttonPress) setSpeed();
  if(track1Next){
    if(track1Index < 1286) track1Index++;
    else{
      digitalWrite(buzzer1, LOW);
      timerAlarmDisable(timer0);
      timerAlarmDisable(timer1);
    }
    if(buzzer1Frequency[track1Index] < 1) buzzer1Frequency[track1Index] = 1;
    if(buzzer1Duration[track1Index] < 1) buzzer1Duration[track1Index] = 1;
    timerAlarmWrite(timer0, (500000.0*speedMultiplayer)/(buzzer1Frequency[track1Index]), true);
    timerAlarmWrite(timer1, ((buzzer1Duration[track1Index]*1000)*speedMultiplayer), true);
    Serial.printf("T1: %d\n", track1Index);
    track1Next = false;
  }
  if(track2Next){
    if(track2Index < 1879) track2Index++;
    else{
      digitalWrite(buzzer2, LOW);
      timerAlarmDisable(timer2);
      timerAlarmDisable(timer3);
    }
    if(buzzer2Frequency[track2Index] < 1) buzzer2Frequency[track2Index] = 1;
    if(buzzer2Duration[track2Index] < 1) buzzer2Duration[track2Index] = 1;
    timerAlarmWrite(timer2, (500000.0*speedMultiplayer)/(buzzer2Frequency[track2Index]), true);
    timerAlarmWrite(timer3, ((buzzer2Duration[track2Index]*1000)*speedMultiplayer), true);
    Serial.printf("T2: %d\n", track2Index);
    track2Next = false;
  }
}
